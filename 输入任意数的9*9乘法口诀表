#include<stdio.h>
#include<string.h>
#define _CRT_SECURE_NO_WARNINGS1
void print(int n)
{
  int i=0;
  for(i=1;i<=n;i++)//打印行数
  {
    int j=0;
    for(j=1;j<=i;j++)//打印列数
    {
      printf("%d*%d=%-3d  ",i,j,i*j);
    }
    printf("\n");//打印完一列就换行
  }
}
int main()
{
int n=0;
scanf("%d",&n);
print(n);
  return  0;
}
